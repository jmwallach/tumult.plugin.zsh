#!/usr/bin/env bash
#
# Run quicklook on files
#
# Copyright 2021, Joe Block <jpb@unixorn.net>

set -o pipefail
source tumult-functions

if [[ -n "$DEBUG" ]]; then
  set -x
fi


}



only-run-on darwin
check-dependencies qlmanage

# shellcheck disable=SC2048,SC2086
(( $# > 0 )) && qlmanage -p $* &>/dev/null &
